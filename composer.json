{
    "name": "lenvanessen/commerce-klarna-payments",
    "description": "Adds Klarna Payments as a payment option for Craft",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "commerce klarna payments"
    ],
    "support": {
        "docs": "https://github.com/lenvanessen/commerce-klarna-payments/blob/master/README.md",
        "issues": "https://github.com/lenvanessen/commerce-klarna-payments/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Len van Essen",
            "homepage": "https://github.com/lenvanessen"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "klarna/kco_rest": "^4.2"
    },
    "autoload": {
        "psr-4": {
          "lenvanessen\\commerce\\klarna\\": "src/"
        }
    },
    "extra": {
        "name": "Commerce Klarna Payments",
        "handle": "commerce-klarna-payments",
        "developer": "Len van Essen",
        "developerUrl": "vanessen.io",
        "documentationUrl": "https://github.com/lenvanessen/commerce-klarna-payments/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/lenvanessen/commerce-klarna-payments/master/CHANGELOG.md",
        "class": "lenvanessen\\commerce\\klarna\\CommerceKlarnaPayments"
    }
}
